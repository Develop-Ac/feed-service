  modulo       String       @db.VarChar
  tela         String       @db.VarChar
  visualizar   Boolean      @default(false)
  editar       Boolean      @default(false)
  criar        Boolean      @default(false)
  deletar      Boolean      @default(false)
  sis_usuarios sis_usuarios @relation(fields: [usuario_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_usuario")
}

model sis_usuarios {
  nome             String
  senha            String
  trash            Int                @default(0)
  id               String             @id
  codigo           String             @unique
  setor            String
  est_auditoria    est_auditoria[]
  est_contagem     est_contagem[]
  est_contagem_log est_contagem_log[]
  sis_permissoes   sis_permissoes[]
  fed_posts       fed_posts[]
  fed_eventos     fed_eventos[]
  fed_comentarios fed_comentarios[]
  fed_curtidas    fed_curtidas[]
  avatar_url       String?
  tema_preferencia String             @default("system")
}


model fed_posts {
  id            String   @id @default(cuid())
  conteudo      String?  @db.Text
  autor_id      String
  tipo          String   @default("texto")
  criado_em     DateTime @default(now())
  atualizado_em DateTime @updatedAt
  curtidas      Int      @default(0)
  
  autor       sis_usuarios    @relation(fields: [autor_id], references: [id])
  midias      fed_midia[]
  comentarios fed_comentarios[]
  likes       fed_curtidas[]

  @@index([autor_id])
}

model fed_eventos {
  id            String   @id @default(cuid())
  titulo        String
  descricao     String?
  data          DateTime @db.Date
  hora          String
  local         String
  tipo          String
  autor_id      String
  criado_em     DateTime @default(now())

  autor         sis_usuarios @relation(fields: [autor_id], references: [id])

  @@index([data])
}

model fed_midia {
  id        String   @id @default(cuid())
  url       String
  tipo      String
  bucket    String   @default("feed")
  chave     String
  post_id   String?
  
  post      fed_posts? @relation(fields: [post_id], references: [id], onDelete: Cascade)
}

model fed_comentarios {
  id        String   @id @default(cuid())
  conteudo  String
  autor_id  String
  post_id   String
  criado_em DateTime @default(now())

  autor     sis_usuarios @relation(fields: [autor_id], references: [id])
  post      fed_posts    @relation(fields: [post_id], references: [id], onDelete: Cascade)
}


model sis_feriados {
  id          String   @id @default(cuid())
  external_id String?
  data        DateTime @db.Date
  nome        String
  tipo        String
  descricao   String?  @db.Text
  uf          String?  @db.VarChar(2)
  codigo_ibge Int?
  ano         Int
  criado_em   DateTime @default(now())

  @@index([data])
  @@index([ano])
  @@index([tipo])
}

model fed_curtidas {
  id        String   @id @default(cuid())
  autor_id  String
  post_id   String
  criado_em DateTime @default(now())

  autor     sis_usuarios @relation(fields: [autor_id], references: [id])
  post      fed_posts    @relation(fields: [post_id], references: [id], onDelete: Cascade)

  @@unique([autor_id, post_id])
  @@index([post_id])
}
