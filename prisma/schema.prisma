model com_pagamento_guia {
  chave_nfe      String   @id
  data_pagamento DateTime @default(now())
  valor          Float    @default(0.0)
  observacoes    String   @default("")
}

model com_pedido {
  id               String             @id
  pedido_cotacao   Int
  for_codigo       Int
  created_at       DateTime           @default(now())
  updated_at       DateTime
  com_pedido_itens com_pedido_itens[]

  @@unique([pedido_cotacao, for_codigo])
  @@index([for_codigo])
  @@index([pedido_cotacao])
}

model com_pedido_itens {
  id             String     @id
  pedido_id      String
  item_id_origem String?
  pro_codigo     Int
  pro_descricao  String
  mar_descricao  String?
  referencia     String?
  unidade        String?
  emissao        DateTime?
  valor_unitario Decimal?
  custo_fabrica  Decimal?
  preco_custo    Decimal?
  for_codigo     Int
  quantidade     Decimal
  created_at     DateTime   @default(now())
  renato         Boolean?   @default(false)
  carlos         Boolean?   @default(false)
  com_pedido     com_pedido @relation(fields: [pedido_id], references: [id], onDelete: Cascade)

  @@unique([pedido_id, pro_codigo, for_codigo])
  @@index([pedido_id])
}

model com_relacionamento_itens {
  id         Int       @id @default(autoincrement())
  group_id   String    @db.VarChar(100)
  pro_codigo String    @unique @db.VarChar(50)
  created_at DateTime? @default(now()) @db.Timestamp(6)

  @@index([group_id], map: "idx_rel_group_id")
  @@index([pro_codigo], map: "idx_rel_pro_codigo")
}

