model ofi_checklists {
  id                           String                   @id
  osInterna                    String?
  dataHoraEntrada              DateTime?
  observacoes                  String?
  combustivelPercentual        Int?
  clienteNome                  String?
  clienteDoc                   String?
  clienteTel                   String?
  clienteEnd                   String?
