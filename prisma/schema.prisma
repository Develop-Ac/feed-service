model com_nfe_conciliacao {
  chave_nfe          String   @id
  emitente           String
  cnpj_emitente      String?
  data_emissao       DateTime
  valor_total        Float
  xml_completo       String?
  status_analise     String   @default("Tributado - Verificado")
  valor_guia         Float    @default(0.0)
  status_erp         String   @default("PENDENTE")
  updated_at         DateTime
