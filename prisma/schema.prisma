generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model com_cotacao {
  id                String              @id
  empresa           Int
  pedido_cotacao    Int                 @unique
  com_cotacao_itens com_cotacao_itens[]
}

model com_cotacao_for {
  id                    String                  @id
  empresa               Int                     @default(3)
  pedido_cotacao        Int
  for_codigo            Int
  for_nome              String?
  cpf_cnpj              String?
  created_at            DateTime                @default(now())
  updated_at            DateTime
  com_cotacao_itens_for com_cotacao_itens_for[]

  @@unique([pedido_cotacao, for_codigo])
  @@index([pedido_cotacao])
}

model com_cotacao_itens {
  pedido_cotacao   Int
  emissao          DateTime?
  pro_codigo       Int
  pro_descricao    String
  mar_descricao    String?
  referencia       String?
  unidade          String?
  quantidade       Int
  dt_ultima_compra DateTime?
  qtd_sugerida     Int?
  com_cotacao      com_cotacao @relation(fields: [pedido_cotacao], references: [pedido_cotacao], onDelete: Cascade)

  @@id([pedido_cotacao, pro_codigo])
}

model com_cotacao_itens_for {
  id              String          @id
  pedido_cotacao  Int
  for_codigo      Int
  emissao         DateTime?
  pro_codigo      Int
  pro_descricao   String
  mar_descricao   String?
  referencia      String?
  unidade         String?
  quantidade      Int
  valor_unitario  Float?
  qtd_sugerida    Int?            @default(0)
  com_cotacao_for com_cotacao_for @relation(fields: [pedido_cotacao, for_codigo], references: [pedido_cotacao, for_codigo], onDelete: Cascade)

  @@index([pedido_cotacao, for_codigo])
}

model com_data_saldo_produto {
  id             Int       @id @default(autoincrement())
  pro_codigo     String?   @db.VarChar(50)
  data_compra    DateTime? @db.Date
  saldo_residual Decimal?  @db.Decimal(15, 4)
  created_at     DateTime? @default(now()) @db.Timestamp(6)

  @@index([pro_codigo], map: "idx_saldo_prod_codigo")
}

model com_fifo_completo {
  id                         Int       @id @default(autoincrement())
  data_processamento         DateTime? @default(now()) @db.Timestamp(6)
  pro_codigo                 String?   @db.VarChar(50)
  tempo_medio_estoque        Decimal?  @db.Decimal(15, 4)
  qtd_vendida                Decimal?  @db.Decimal(15, 4)
  valor_vendido              Decimal?  @db.Decimal(15, 4)
  data_min_venda             DateTime? @db.Date
  data_max_venda             DateTime? @db.Date
  periodo_dias               Int?
  demanda_media_dia          Decimal?  @db.Decimal(15, 6)
  num_vendas                 Int?
  vendas_ult_12m             Decimal?  @db.Decimal(15, 4)
  vendas_12m_ant             Decimal?  @db.Decimal(15, 4)
  fator_tendencia            Decimal?  @db.Decimal(10, 6)
  tendencia_label            String?   @db.VarChar(20)
  dias_ruptura               Int?
  demanda_media_dia_ajustada Decimal?  @db.Decimal(15, 6)
  pro_descricao              String?
  estoque_disponivel         Decimal?  @db.Decimal(15, 4)
  mar_descricao              String?   @db.VarChar(100)
  fornecedor1                String?   @db.VarChar(200)
  fornecedor2                String?   @db.VarChar(200)
  fornecedor3                String?   @db.VarChar(200)
  pct_acum_valor             Decimal?  @db.Decimal(10, 4)
  curva_abc                  String?   @db.VarChar(10)
  categoria_estocagem        String?   @db.VarChar(20)
  estoque_min_base           Int?
  estoque_max_base           Int?
  fator_ajuste_tendencia     Decimal?  @db.Decimal(10, 6)
  estoque_min_ajustado       Int?
  estoque_max_ajustado       Int?
  estoque_min_sugerido       Int?
  estoque_max_sugerido       Int?
  tipo_planejamento          String?   @db.VarChar(30)
  alerta_tendencia_alta      String?   @db.VarChar(10)
  descricao_calculo_estoque  String?
  created_at                 DateTime? @default(now()) @db.Timestamp(6)
  sgr_codigo                 Int?
  teve_alteracao_analise     Boolean?  @default(false)
  dados_alteracao_json       String?
  sgr_descricao              String?   @db.VarChar(200)
  group_id                   String?   @db.VarChar(100)
  grp_estoque_disponivel     Decimal?  @db.Decimal(15, 4)
  grp_qtd_vendida            Decimal?  @db.Decimal(15, 4)
  grp_valor_vendido          Decimal?  @db.Decimal(15, 4)
  grp_num_vendas             Int?
  grp_vendas_ult_12m         Decimal?  @db.Decimal(15, 4)
  grp_vendas_12m_ant         Decimal?  @db.Decimal(15, 4)
  grp_estoque_min_base       Int?
  grp_estoque_max_base       Int?
  grp_estoque_min_ajustado   Int?
  grp_estoque_max_ajustado   Int?
  grp_estoque_min_sugerido   Int?
  grp_estoque_max_sugerido   Int?
  grp_demanda_media_dia      Decimal?  @db.Decimal(15, 6)
  rateio_prop_grupo          Decimal?  @db.Decimal(10, 6)
  tempo_medio_saldo_atual    Decimal?  @db.Decimal(15, 2)
  categoria_saldo_atual      String?   @db.VarChar(50)

  @@index([teve_alteracao_analise], map: "idx_com_fifo_alteracao")
  @@index([categoria_estocagem], map: "idx_com_fifo_categoria_estocagem")
  @@index([categoria_saldo_atual], map: "idx_com_fifo_categoria_saldo_atual")
  @@index([curva_abc], map: "idx_com_fifo_curva_abc")
  @@index([data_processamento], map: "idx_com_fifo_data_processamento")
  @@index([group_id], map: "idx_com_fifo_group_id")
  @@index([pro_codigo], map: "idx_com_fifo_pro_codigo")
}

model com_kanban {
  id        String   @id
  data      Json
  updatedAt DateTime
  createdAt DateTime @default(now())
}

model com_metricas_fifo {
  pro_codigo                Int       @id(map: "pk_metricas_estoque_fifo")
  tempo_medio_estoque       Decimal?  @db.Decimal(18, 6)
