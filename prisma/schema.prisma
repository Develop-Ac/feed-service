  categoria_estocagem       String?   @db.VarChar(20)
  estoque_min_base          Int?
  estoque_max_base          Int?
  fator_ajuste_tendencia    Decimal?  @db.Decimal(18, 6)
  estoque_min_ajustado      Int?
  estoque_max_ajustado      Int?
  estoque_min_sugerido      Int?
  estoque_max_sugerido      Int?
  tipo_planejamento         String?   @db.VarChar(30)
  alerta_tendencia_alta     String?   @db.VarChar(3)
